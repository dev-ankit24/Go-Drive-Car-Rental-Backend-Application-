{{>header}}
<title>Go Drive - Car Rental | Contact Us</title>
{{>navbar}}
{{>hero}}

<!-- Contact Start -->
<div class="container-fluid contact py-5">
    <div class="container py-5">
        <div class="text-center mx-auto pb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 800px;">
            <h1 class="display-5 text-capitalize text-primary mb-3">Contact Us</h1>
        </div>
        <div class="row g-5">
            <div class="col-12 wow fadeInUp" data-wow-delay="0.1s">
                <div class="row g-5">
                    <div class="col-md-6 col-lg-6 col-xl-3">
                        <div class="contact-add-item p-4">
                            <div class="contact-icon mb-4">
                                <i class="fas fa-map-marker-alt text-light fa-2x"></i>
                            </div>
                            <div>
                                <h4>Address</h4>
                                <p class="mb-0">Noida , Uttar Pradesh, India</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-xl-3 wow fadeInUp" data-wow-delay="0.3s">
                        <div class="contact-add-item p-4">
                            <div class="contact-icon mb-4">
                                <i class="fas fa-envelope text-light fa-2x"></i>
                            </div>
                            <div>
                                <h4>Mail Us</h4>
                                <p class="mb-0  "><a class="text-dark"
                                        href="mailto:webankit242@gmail.com">webankit242@gmail.com</a></p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-xl-3 wow fadeInUp" data-wow-delay="0.5s">
                        <div class="contact-add-item p-4">
                            <div class="contact-icon mb-4">
                                <i class="fa fa-phone-alt text-light fa-2x"></i>
                            </div>
                            <div>
                                <h4>Telephone</h4>
                                <p class="mb-0"><a class="text-dark" href="tel:+918392823395">+91 8392823395</a></p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-xl-3 wow fadeInUp" data-wow-delay="0.7s">
                        <div class="contact-add-item p-4">
                            <div class="contact-icon mb-4">
                                <i class="fab fa-whatsapp text-light fa-2x"></i>
                            </div>
                            <div>
                                <h4>Whats App</h4>
                                <p class="mb-0"><a class="text-dark" href="https://wa.me/+918392823395">+91
                                        8392823395</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6 wow fadeInUp" data-wow-delay="0.1s">
                <div class="bg-secondary p-5 rounded">
                    <h3 class="text-light text-center mb-4">{{#if show}}Thanks to Share Your Query with Us . Our Team
                        wil Contact You Soon !!! {{else}}Send Your Message{{/if}}</h3>
                    <form action="" method="post">
                        <div class="row g-4">
                            <div class="col-lg-12 col-xl-12">
                                <div class="form-floating">
                                    <input type="text" name="name"
                                        class="form-control {{#if errorMessage.name}} border-danger border-6{{/if}}"
                                        id="name" placeholder="Your Name">
                                    <label for="name">{{#if errorMessage.name }}{{errorMessage.name }}{{else}}Your
                                        Name{{/if}}</label>
                                </div>
                            </div>

                            <div class="col-lg-12 col-xl-6">
                                <div class="form-floating">
                                    <input type="email" name="email"
                                        class="form-control{{#if errorMessage.email}}border-danger  border-6{{/if}}"
                                        id="email" placeholder="Your Email">
                                    <label for="email">{{#if errorMessage.email}}{{errorMessage.email}} {{else}}Your
                                        Email{{/if}}</label>
                                </div>
                            </div>
                            <div class="col-lg-12 col-xl-6">
                                <div class="form-floating">
                                    <input type="phone" name="phone"
                                        class="form-control {{#if errorMessage.phone}}border-danger border-6 {{/if}}"
                                        id="phone" placeholder="Phone">
                                    <label for="phone">{{#if errorMessage.phone}}{{errorMessage.phone}}{{else}}Your
                                        Phone{{/if}}</label>
                                </div>
                            </div>

                            <div class="col-12">
                                <div class="form-floating">
                                    <input type="text" name="subject"
                                        class="form-control {{#if errorMessage.subject}}border-danger border-6 {{/if}}"
                                        id="subject" placeholder="Subject">
                                    <label for="subject"> {{#if
                                        errorMessage.subject}}{{errorMessage.subject}}{{else}}Subject{{/if}}</label>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating">
                                    <textarea
                                        class="form-control {{#if errorMessage.message}}border-danger border-6 {{/if}}"
                                        name="message" placeholder="Leave a message here" id="message"
                                        style="height: 160px"></textarea>
                                    <label for="message">{{#if
                                        errorMessage.message}}{{errorMessage.message}}{{else}}Message{{/if}}</label>
                                </div>

                            </div>
                            <div class="col-12">
                                <button class="btn btn-light w-100 py-3">Send Message</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-12 col-xl-1 wow fadeInUp" data-wow-delay="0.3s">
                <div class="d-flex flex-xl-column align-items-center justify-content-center">
                    <a class="btn btn-xl-square btn-light rounded-circle mb-0 mb-xl-4 me-4 me-xl-0" target="_blank" href="https://bit.ly/4dVBSrK"><i
                            class="fab fa-facebook-f"></i></a>
                    <a class="btn btn-xl-square btn-light rounded-circle mb-0 mb-xl-4 me-4 me-xl-0" target="_blank" href="https://bit.ly/3AB290e"><i
                            class="fab fa-twitter"></i></a>
                    <a class="btn btn-xl-square btn-light rounded-circle mb-0 mb-xl-4 me-4 me-xl-0" target="_blank" href="https://bit.ly/4dVBSrK"><i
                            class="fab fa-instagram"></i></a>
                    <a class="btn btn-xl-square btn-light rounded-circle mb-0 mb-xl-0 me-0 me-xl-0" target="_blank" href="https://www.linkedin.com/in/kumarankit24/"><i
                            class="fab fa-linkedin-in"></i></a>
                </div>
            </div>
            <div class="col-12 col-xl-5 wow fadeInUp" data-wow-delay="0.1s">
                <div class="p-1 bg-light rounded">

                    <iframe width="100%" height="560px" id="gmap_canvas"
                        src="https://maps.google.com/maps?q=A-43%2C+sector16+noida&t=&z=13&ie=UTF8&iwloc=&output=embed"
                        frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
                </div>

            </div>
        </div>


    </div>
</div>
</div>
{{>footer}}
<!-- Contact End -->